---
import { Button } from "@ui/components/button/button";
import { CloudOff, Moon } from "lucide-react"

---
<header class="sticky top-0 left-0 z-10 bg-woodsmoke-950/90 backdrop-blur m-auto min-h-min max-h-min my-10 w-3/5 transition-all">
  <nav class="w-auto flex items-center justify-between">
    <Button variant="raw" size="icon">
      <CloudOff size={28}/>
    </Button>
    <div class="inline-flex items-center justify-start space-x-2">
      <Button variant="raw" size="icon">
        <Moon size={28}/>
      </Button>
    </div>
  </nav>
</header>