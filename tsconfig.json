{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [ "src/pages/*" ],
      "@styles/*": [ "src/styles/*" ],
      "@layouts/*": [ "src/layouts/*" ],
      "@utils/*": [ "src/utils/*" ],
      "@ui/*":[ "src/ui/*" ],
      "@api/*": [ "src/api/*" ],
      "@constant/*": [ "src/constant/*" ],
      "@libs/*": ["src/libs/*"],
      "@hooks/*": ["src/hooks/*"],
      "@templates/*": ["src/templates/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}